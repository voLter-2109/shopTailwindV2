{
  "singleQuote": true,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "jsxSingleQuote": true,
  "semi": false,
  "useTabs": true,
  "trailingComma": "none",

  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/layout/(.*)$",
    "^@/ui/(.*)$",
    "^@/constant/(.*)$",
    "^@/services/(.*)$",
    "^@/providers/(.*)$",
    "^@/shared/(.*)$",
    "^@/assets/(.*)$",
    "^@/config/(.*)$",
    "^@/store/(.*)$",
    "^@/hooks/(.*)$",
    "^@/types/(.*)$",
    "^@/utils/(.*)$",
    "^@/api/(.*)$",
    "^@../(.*)$",
    "^@./(.*)$"
  ]
}
